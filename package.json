{
  "name": "react-js-quiz",
  "private": true,
  "version": "2025.5.0",
  "type": "module",
  "description": "Challenge your knowledge with Quizzly.dev! Test yourself across web development, JavaScript, TypeScript, and modern frameworks like React, Vue, Angular, and Svelte. Features real-time scoring, instant feedback, and shareable results. Perfect for developers and tech enthusiasts.",
  "author": {
    "name": "Mahak & Vedansh",
    "url": "https://github.com/offensive-vk/reactjs-quiz-app",
    "bugs": "https://github.com/offensive-vk/reactjs-quiz-app/issues/new"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "vite --host --port 17027 --force --clearScreen -d",
    "build": "vite build -d --sourcemap && node replace-sha.mjs",
    "preview": "vite preview -d",
    "optimize": "vite optimize -d",
    "lint": "eslint .",
    "all": "pnpm run build; pnpm run optimize",
    "format": "deno fmt"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm.onlyBuiltDependencies": "esbuild",
  "dependencies": {
    "bootstrap": "^5.3.6",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "html2canvas": "^1.4.1",
    "react-error-boundary": "^5.0.0",
    "react-markdown": "^10.1.0",
    "vite": "^6.3.5"
  }
}
